<%= form_with url: "/cars/#{@car.id}", method: :patch, local: true do |form| %>
<%= form.label :make %>
<%= form.text_field :make %><br>
<%= form.label :model %>
<%= form.text_field :model %><br>
<%= form.label :awd %>
<%= form.text_field :awd %><br>
<%= form.label :mileage %>
<%= form.text_field :mileage %><br>
<%= form.submit "Edit Car" %>
<% end %>